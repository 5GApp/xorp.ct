	    multicast: true
	}
*/
    }
}

plumbing {
    mfea4 {
	enabled: true
	interface dc0 {
	    vif dc0 {
		enabled: true
	    }
	}
	interface register_vif {
	    vif register_vif {
		/* Note: this vif should be always enabled */
		enabled: true
	    }
	}
	traceoptions {
	    flag all {
		enabled: true
	    }
	}
    }

