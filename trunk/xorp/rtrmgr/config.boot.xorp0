/* $XORP: xorp/rtrmgr/config.boot.xorp0,v 1.3 2004/05/13 00:01:55 pavlin Exp $ */

/* this is the test config for xorp0.icir.org */

interfaces {
  interface fxp0 {
    description: "control interface"
    /* default_system_config */
    vif fxp0 {
      address 192.150.187.100 {
	prefix-length: 24
	broadcast: 192.150.187.255
      }
    }
  }
}

fea {
    enable_unicast_forwarding4: true
    /* enable_unicast_forwarding6: true */
}

/*
