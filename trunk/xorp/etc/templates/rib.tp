/* $XORP: xorp/etc/templates/rib.tp,v 1.11 2004/05/16 07:15:17 pavlin Exp $ */

plumbing {
    rib {
	targetname: txt = "rib";
    }
}

plumbing {
    rib {
	%modinfo: provides rib;
	%modinfo: depends fea;
	%modinfo: path "rib/xorp_rib";
	%modinfo: default_targetname "rib";
	%modinfo: statusmethod xrl "$(rib.targetname)/common/0.1/get_status->status:u32&reason:txt";
	%modinfo: shutdownmethod xrl "$(rib.targetname)/common/0.1/shutdown";

	targetname {
	    %set:;
	}
    }
}
