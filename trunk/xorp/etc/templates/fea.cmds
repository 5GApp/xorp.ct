    %command: "fea/tools/show_interfaces" %help: HELP;
    %module: fea;
    %tag: HELP "Show network interface information";
}

show interfaces $(interfaces.interface.*) {
    %command: "fea/tools/show_interfaces" %help: HELP;
    %module: fea;
    %tag: HELP "Show information about a single network interface";
}
