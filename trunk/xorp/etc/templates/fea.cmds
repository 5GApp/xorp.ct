show interface $(interfaces.interface.*) {
	%command: rtrmgr/tools/show_interfaces;
	%module: fea;
	%opt_parameter: brief;
	%opt_parameter: detail;
	%opt_parameter: extensive;
}
show vif $(interfaces.interface.*.vif.*) {
	%command: rtrmgr/tools/show_interfaces;
	%module: fea;
	%opt_parameter: brief;
	%opt_parameter: detail;
	%opt_parameter: extensive;
}
