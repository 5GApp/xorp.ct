SubDir TOP mrt ;
if $(MAKE_CHECK) {
  SubIncludeOnce TOP libxorp ;
}
SubDir TOP mrt ;

Library libmrt : 
	buffer.c
	random.c
	mifset.cc
	mrib_table.cc
	;

if $(MAKE_CHECK) {
 Main test_mrt : test_mrt.cc ;
 Main test_mrib : test_mrib.cc ;
 LinkLibraries test_mrt test_mrib : libmrt libxorp ;
}
