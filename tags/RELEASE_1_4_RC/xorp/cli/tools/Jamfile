LinkLibraries send_cli_processor_xrl :
	libcliprocessorxif
	libxipc
	libcomm
	libxorp
        ;

# XXX: $(PREFIX)$(SUBDIR) introduces a period.
InstallBin $(PREFIX)/cli/tools : send_cli_processor_xrl ;
