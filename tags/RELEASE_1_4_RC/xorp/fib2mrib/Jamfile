SubDir TOP fib2mrib ;
SubIncludeOnce TOP libcomm ;
SubIncludeOnce TOP libxorp ;
SubIncludeOnce TOP xrl interfaces ;
SubIncludeOnce TOP xrl targets ;
SubIncludeOnce TOP libxipc ;
SubIncludeOnce TOP libfeaclient ;
SubIncludeOnce TOP policy backend ;
SubIncludeOnce TOP policy common ;
SubDir TOP fib2mrib ;

Library libfib2mrib :
	fib2mrib_node.cc
	fib2mrib_varrw.cc
	xrl_fib2mrib_node.cc
	;

Main xorp_fib2mrib : xorp_fib2mrib.cc ;
LinkLibraries xorp_fib2mrib :
	libfib2mrib
	libfib2mribbase
	libpolicybackend
	libpolicycommon
	libfeaclient
	libfindereventnotifierxif
	libfeaifmgrmirrorxif
	libfeaifmgrreplicatorxif
	libftixif
	libfeafibxif
	libribxif
	libfeaifmgrmirrorbase
	libxipc
	libcomm
	libxorp
	;

InstallBin $(PREFIX)/fib2mrib : xorp_fib2mrib ;
