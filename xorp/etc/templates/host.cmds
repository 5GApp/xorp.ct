/* $XORP: xorp/etc/templates/host.cmds,v 1.3 2006/02/22 02:07:31 pavlin Exp $ */

show host {
    %command: "" %help: HELP;
    %tag: HELP "Display information about the host";
}

show host date {
    %command: "date" %help: HELP;
    %tag: HELP "Show host current date";
}

show host name {
    %command: "hostname" %help: HELP;
    %tag: HELP "Show host name";
}

show host os {
    %command: "uname -a" %help: HELP;
    %tag: HELP "Show host operating system details";
}
